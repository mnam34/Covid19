<!DOCTYPE html>
<!--[if IE 8]> <html class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html>
<!--<![endif]-->
<head>
    <meta charset="utf-8" />
    <title>@Html.Raw(ViewData["Title"])</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="Hệ quản trị nội dung CMS" name="description" />
    <meta content="mvnam.tho@gmail.com" name="author" />
    <link href="~/themes/m475/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/themes/m475/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/themes/m475/assets/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/themes/m475/assets/global/plugins/select2/css/select2.css" rel="stylesheet" type="text/css" />
    <link href="~/themes/m475/assets/global/plugins/select2/css/select2-bootstrap.min.css" rel="stylesheet" type="text/css" />
    @RenderSection("styles", required: false)
    <link href="~/themes/m475/assets/global/css/components-md.min.css" asp-append-version="true" rel="stylesheet" id="style_components" type="text/css" />
    <link href="~/themes/m475/assets/global/css/plugins-md.min.css" asp-append-version="true" rel="stylesheet" type="text/css" />
    <link href="~/themes/m475/assets/layouts/layout3/css/layout.min.css" asp-append-version="true" rel="stylesheet" type="text/css" />
    <link href="~/themes/m475/assets/layouts/layout3/css/themes/default.min.css" asp-append-version="true" rel="stylesheet" type="text/css" id="style_color" />
    <link href="~/themes/m475/assets/layouts/layout3/css/custom-ctct.css" asp-append-version="true" rel="stylesheet" type="text/css" />
    @RenderSection("styles2", required: false)
    <link rel="shortcut icon" href="~/imgs/logo2.png" />
</head>
<body class="page-container-bg-solid page-md">
    <div class="page-wrapper">
        <div class="page-wrapper-row">
            <div class="page-wrapper-top">
                <!-- BEGIN HEADER -->
                <div class="page-header">
                    <!-- BEGIN HEADER TOP -->
                    <div class="page-header-top">
                        <div class="container-fluid">
                            <!-- BEGIN LOGO -->
                            <div class="page-logo">
                                <a asp-controller="Home" asp-action="Index" asp-area="SM">
                                    <img src="~/imgs/logo2.png" alt="NamMV" class="logo-default">
                                </a>
                            </div>
                            <div class="site-title">
                                @*<h1 class="text-danger">Covid-19 F Case Management</h1>*@
                                <h3 class="margin-top-5 page-title-h3">
                                        <span class="hidden-480">
                                            <img src="~/imgs/covid.png" alt="Covid-19 F Case Management" />
                                        </span>
                                    </h3>
                            </div>
                            <!-- END LOGO -->
                            @{ await Html.RenderPartialAsync("_UserPartial"); }
                        </div>
                    </div>
                    <!-- END HEADER TOP -->
                    <!-- BEGIN HEADER MENU -->
                    <div class="page-header-menu">
                        <div class="container-fluid">
                            <!-- BEGIN MEGA MENU -->
                            <!-- DOC: Apply "hor-menu-light" class after the "hor-menu" class below to have a horizontal menu with white background -->
                            <!-- DOC: Remove data-hover="dropdown" and data-close-others="true" attributes below to disable the dropdown opening on mouse hover -->
                            <div class="hor-menu  ">
                                <ul class="nav navbar-nav">
                                    <li aria-haspopup="true" class="menu-dropdown classic-menu-dropdown ">
                                        <a asp-controller="Home" asp-action="Index" asp-area="SM">Trang chủ</a>
                                    </li>
                                    <li class="@Html.IsSelected("active","SM","Chart","Index")">
                                        <a asp-controller="Chart" asp-action="Index" asp-area="SM" class="@Html.IsSelected("active","SM","Chart","Index")">1. Biểu đồ</a>
                                    </li>
                                    <li class="@Html.IsSelected("active","SM","FCase","Create")">
                                        <a asp-controller="FCase" asp-action="Create" asp-area="SM" class="@Html.IsSelected("active","SM","FCase","Create")">2. Nhập ca bệnh F0</a>
                                    </li>
                                    <li class="@Html.IsSelected("active","SM","FCase","Index,CreateFx,Update,UpdateFx,Detail")">
                                        <a asp-controller="FCase" asp-action="Index" asp-area="SM" class="@Html.IsSelected("active","SM","FCase","Index,CreateFx,Update,UpdateFx,Detail")">3. Danh sách ca bệnh F0</a>
                                    </li>
                                    <li aria-haspopup="true" class="menu-dropdown classic-menu-dropdown @Html.SelectCurrentController("active", "SM", "Report")">
                                        <a href="javascript:;">4. Báo cáo<span class="arrow"></span></a>
                                        <ul class="dropdown-menu pull-left">
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","Report","FCase")">
                                                <a asp-controller="Report" asp-action="FCase" asp-area="SM" class="nav-link @Html.IsSelected("active","SM","Report","FCase")">Báo cáo </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","Report","FCaseChart2")">
                                                <a asp-controller="Report" asp-action="FCaseChart2" asp-area="SM" class="nav-link @Html.IsSelected("active","SM","Report","FCaseChart2")">Biểu đồ </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","Report","FCaseChart")">
                                                <a asp-controller="Report" asp-action="FCaseChart" asp-area="SM" class="nav-link @Html.IsSelected("active","SM","Report","FCaseChart")">Biểu đồ (draft) </a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li aria-haspopup="true" class="menu-dropdown classic-menu-dropdown @Html.SelectCurrentController("active", "SM", "EpidemicArea,QuarantinePlace,TreatmentFacility,DetectedPlace,QuarantineType,RiskClassification")">
                                        <a href="javascript:;">5. Danh mục<span class="arrow"></span></a>
                                        <ul class="dropdown-menu pull-left">
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","EpidemicArea","Index,Create,Update")">
                                                <a href="@Url.RouteUrl("EpidemicAreaIndex")" class="nav-link @Html.IsSelected("active","SM","EpidemicArea","Index,Create,Update")">5.1 Danh mục Khu vực/vùng/điểm bùng phát </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","TreatmentFacility","Index,Create,Update")">
                                                <a href="@Url.RouteUrl("TreatmentFacilityIndex")" class="nav-link @Html.IsSelected("active","SM","TreatmentFacility","Index,Create,Update")">5.2 Danh mục Cơ sở điều trị bệnh </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","QuarantinePlace","Index,Create,Update")">
                                                <a href="@Url.RouteUrl("QuarantinePlaceIndex")" class="nav-link @Html.IsSelected("active","SM","QuarantinePlace","Index,Create,Update")">5.3 Danh mục Khu cách ly tập trung </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","DetectedPlace","Index,Create,Update")">
                                                <a href="@Url.RouteUrl("DetectedPlaceIndex")" class="nav-link @Html.IsSelected("active","SM","DetectedPlace","Index,Create,Update")">5.4 Danh mục Nơi phát hiện ca bệnh </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","QuarantineType","Index,Create,Update")">
                                                <a href="@Url.RouteUrl("QuarantineTypeIndex")" class="nav-link @Html.IsSelected("active","SM","QuarantineType","Index,Create,Update")">5.5 Danh mục Hình thức cách ly </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","RiskClassification","Index,Create,Update")">
                                                <a href="@Url.RouteUrl("RiskClassificationIndex")" class="nav-link @Html.IsSelected("active","SM","RiskClassification","Index,Create,Update")">5.6 Danh mục Mức độ nguy cơ nhiễm bệnh </a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li aria-haspopup="true" class="menu-dropdown classic-menu-dropdown @Html.SelectCurrentController("active", "SM", "Account,Role,Summary")">
                                        <a href="javascript:;">6. Quản trị<span class="arrow"></span></a>
                                        <ul class="dropdown-menu pull-left">
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","Account","Index,Create,Read")">
                                                <a href="@Url.RouteUrl("Account_Index")" class="nav-link @Html.IsSelected("active","SM","Account","Index,Create,Read")">6.1 Người dùng </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","Role","Index,Create,Update,ModuleRole")">
                                                <a href="@Url.RouteUrl("RoleIndex")" class="nav-link @Html.IsSelected("active","SM","Role","Index,Create,Update,ModuleRole")">6.2 Nhóm quyền </a>
                                            </li>
                                            <li aria-haspopup="true" class="@Html.IsSelected("active","SM","Summary","AssignedRole")">
                                                <a href="@Url.RouteUrl("SummaryAssignedRole")" class="nav-link @Html.IsSelected("active","SM","Summary","AssignedRole")">6.3 Người dùng theo Nhóm quyền </a>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                            <!-- END MEGA MENU -->
                        </div>
                    </div>
                    <!-- END HEADER MENU -->
                </div>
                <!-- END HEADER -->
            </div>
        </div>
        <div class="page-wrapper-row full-height">
            <div class="page-wrapper-middle">
                <!-- BEGIN CONTAINER -->
                <div class="page-container">
                    <!-- BEGIN CONTENT -->
                    <div class="page-content-wrapper">
                        <!-- BEGIN CONTENT BODY -->
                        <!-- BEGIN PAGE HEAD-->
                        @*<div class="page-head">
                                <div class="container-fluid">
                                    <div class="page-title">
                                        <h1>Blank Page </h1>
                                    </div>
                                </div>
                            </div>*@
                        <!-- END PAGE HEAD-->
                        <!-- BEGIN PAGE CONTENT BODY -->
                        <div class="page-content">
                            <div class="container-fluid">
                                <!-- BEGIN PAGE BREADCRUMBS -->
                                @*<ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a asp-controller="Home" asp-action="Index" asp-area="SM">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                            <span>Layouts</span>
                                        </li>
                                    </ul>*@
                                <!-- END PAGE BREADCRUMBS -->
                                <!-- BEGIN PAGE CONTENT INNER -->
                                <div class="page-content-inner">
                                    @RenderBody()
                                </div>
                                <!-- END PAGE CONTENT INNER -->
                            </div>
                        </div>
                        <!-- END PAGE CONTENT BODY -->
                        <!-- END CONTENT BODY -->
                    </div>
                    <!-- END CONTENT -->
                </div>
                <!-- END CONTAINER -->
            </div>
        </div>
        <div class="page-wrapper-row">
            <div class="page-wrapper-bottom">
                <div class="page-footer">
                    <div class="container-fluid">
                        @if (DateTime.Now.Year <= 2021)
                        {
                            <span>2021</span>
                        }
                        else
                        {
                            <span>2021-@DateTime.Now.Year</span>
                        }
                        &copy;
                        NamMV
                    </div>
                </div>
                <div class="scroll-to-top">
                    <i class="icon-arrow-up"></i>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="Success_Message_Holder" value="@TempData["Success"]" />
    <input type="hidden" id="Error_Message_Holder" value="@TempData["Error"]" />
    @*@{ await Html.RenderPartialAsync("_AppParital"); }*@
    @RenderSection("ModalContainer", required: false)
    @RenderSection("ServerValueHolder", required: false)
    <!--[if lt IE 9]>
    <script src="~/themes/m475/assets/global/plugins/respond.min.js"></script>
    <script src="~/themes/m475/assets/global/plugins/excanvas.min.js"></script>
    <script src="~/themes/m475/assets/global/plugins/ie8.fix.min.js"></script>
    <![endif]-->
    <script src="~/themes/m475/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/themes/m475/assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/themes/m475/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/themes/m475/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/themes/m475/assets/global/plugins/select2/js/select2.min.js" type="text/javascript"></script>
    <script src="~/lib/sweetalert.min.js" type="text/javascript"></script>
    @RenderSection("Scripts", required: false)
    <script src="~/themes/m475/assets/global/scripts/app.js" asp-append-version="true" type="text/javascript"></script>
    @RenderSection("Scripts2", required: false)
    <script src="~/themes/m475/assets/layouts/layout3/scripts/layout.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/themes/m475/assets/layouts/global/scripts/quick-nav.min.js" asp-append-version="true" type="text/javascript"></script>
</body>

</html>



