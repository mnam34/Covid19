@model IEnumerable<Report1>
@{
    ViewData["Title"] = "Báo cáo tổng hợp F-Case";
    var barData = ViewData["groups"] as List<BarChartModel>;
    var dateOk = barData.Select(o => o.Date).ToList();
    var dates1 = string.Join(",", barData.Select(o => o.Date));
    var dates2 = string.Join(",", barData.Select(x => $"'{x.Date}'"));
    var dates = string.Join(",", barData.Select(x => string.Format("'{0}'", x.Date)));
    var datas = string.Join(",", barData.Select(o => o.Data));
}
<div class="row">
    <div class="col-md-12">
        <div class="portlet light portlet-fit portlet-datatable bordered">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-puzzle-piece font-green-sharp "></i>
                    <span class="caption-subject font-green-sharp bold ">@Html.Raw(ViewData["Title"])</span>
                </div>
                <div class="actions">
                    @*<a href="javascript:;" data-action="0" class="tool-action btn green-jungle"><i class="fa fa-file-excel-o"></i> Kết xuất</a>*@
                </div>
            </div>
            <div class="portlet-body">
                <h4>Số ca dương tính phân theo ngày</h4>
                <div id="fcase-e-chart-bar1" style="height:500px;"></div>
                <div id="fcase-e-chart-line1" style="height:500px;"></div>
            </div>

        </div>
    </div>
</div>
<input type="hidden" id="dates" value="@Html.Raw(dates)" />
<input type="hidden" id="datas" value="@datas" />
@section Styles{

}
@section Scripts {
    <script src="~/themes/m475/assets/global/plugins/echarts/echarts.js" type="text/javascript"></script>
}
@section Scripts2 {

    <script src="~/themes/m475/assets/pages/scripts/report-fcase-chart.js" type="text/javascript"></script>
}
